{
  "name": "mars_rover",
  "version": "1.0.0",
  "description": "Latest updates from NASA's Mars Rover Perserverance",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "server": "node server.js",
    "client": "npm run start --prefix client",
    "install-client": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run install-client && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "https://github/diuguide/marsRover_updates.git"
  },
  "keywords": [
    "Mars",
    "Space",
    "NASA",
    "JPL",
    "Everett",
    "Diuguid",
    "Diuguid",
    "Perserverance",
    "Rover",
    "Exploration"
  ],
  "author": "Everett Diuguid",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^6.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.15",
    "nodemon": "^2.0.7"
  }
}
